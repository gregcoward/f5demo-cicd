{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
  },
  "variables": {
    "storageAccountType": "Standard_LRS",
    "dnsLabel": "[toLower(concat('f5demo-',substring(uniqueString(resourceGroup().id),0,5)))]",
    "publicIPAddressID": "[resourceId('Microsoft.Network/publicIPAddresses',concat(parameters('namePrefix'), 'pip'))]",
    "lbID": "[resourceId('Microsoft.Network/loadBalancers',concat(parameters('namePrefix'), 'lb'))]",
    "location": "[resourceGroup().location]"
  },
  "resources": [
    {
      "type": "Microsoft.Compute/availabilitySets",
      "tags": {
        "displayName": "Availability Set"
      },
      "name": "[concat(variables('dnsLabel'), '-app-as')]",
      "apiVersion": "2015-05-01-preview",
      "location": "[variables('location')]",
      "properties": {}
    }
  ],
  "outputs": {
    "Infrastructure Deployment Status": {
      "type": "string",
      "value": "Successful"
    }
  }
}
